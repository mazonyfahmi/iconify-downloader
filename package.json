{
  "name": "iconify-downloader",
  "version": "0.0.3",
  "description": "Download icons as SVG and JSON from the Iconify API",
  "author": "Fahmi Farid",
  "license": "MIT",
  "type": "module",
  "bin": {
    "iconify-downloader": "dist/cli.cjs"
  },
  "main": "dist-electron/main.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "iconify",
    "icons",
    "svg",
    "json",
    "icon-downloader"
  ],
  "scripts": {
    "dev": "concurrently -k \"vite\" \"npm run dev:electron\"",
    "dev:electron": "wait-on tcp:4000 && tsc -p tsconfig.electron.json && node -e \"require('fs').writeFileSync('dist-electron/package.json', '{\\\"type\\\": \\\"commonjs\\\"}')\" && electron .",
    "build": "tsc -p tsconfig.electron.json && node -e \"require('fs').writeFileSync('dist-electron/package.json', '{\\\"type\\\": \\\"commonjs\\\"}')\" && vite build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "archiver": "^7.0.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.28.1",
    "fs-extra": "^11.3.0",
    "lucide-react": "^0.562.0",
    "node-html-parser": "^7.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/archiver": "^6.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "commander": "^14.0.0",
    "concurrently": "^9.2.1",
    "daisyui": "^5.5.14",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^7.3.1",
    "wait-on": "^9.0.3"
  },
  "engines": {
    "node": "20"
  }
}
